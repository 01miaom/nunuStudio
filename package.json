{
	"name": "nunuStudio",
	"description": "nunuStudio is a web based game engine for 3D and 2D game development with support for VR and AR",
	"main": "source/editor.html",
	"directories": {
		"doc": "docs"
	},
	"scripts": {
		"start": "nwb nwbuild --with-ffmpeg -v 0.33.4-sdk -r ./docs/editor",
		"build-desktop": "nwb nwbuild --executable-name nunuStudio --with-ffmpeg -v 0.33.4-sdk -p win64 --win-ico ./source/favicon.ico --side-by-side -o ./build ./docs/editor",
		"build": "cd build/build && node build.js",
		"update": "cd build/build && node update.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tentone/nunuStudio.git"
	},
	"keywords": [
		"three.js",
		"editor",
		"webgl",
		"engine",
		"gamedev",
		"cannonjs"
	],
	"author": "Tentone",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/tentone/nunuStudio/issues"
	},
	"homepage": "https://www.nunustudio.org",
	"devDependencies": {
		"yuidocjs": "",
		"nwjs-builder": ""
	},
	"dependencies": {
		"npm": "^6.4.1"
	},
	"nwjsBuilder": {
		"productName": "nunuStudio",
		"productVersion": "",
		"fileDescription": "",
		"copyright": "Tentone",
		"internalName": "",
		"fileVersion": "",
